{
  "name": "azhovan/sms1000",
  "description": "SMS1000 Notifications channel for Laravel",
  "homepage": "https://github.com/Azhovan/Sms1000",
  "license": "MIT",
  "authors": [
    {
      "name": "Azhovan Asadi",
      "email": "elazhi@outlook.com",
      "homepage": "https://github.com/Azhovan"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/notifications": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/queue": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Sms1000\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sms1000\\SmsChannelServiceProvider"
      ]
    }
  }
}
